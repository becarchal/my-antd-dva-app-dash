{
  "name": "my-antd-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://becarchal.github.io/my-antd-app/",
  "dependencies": {
    "antd": "^2.13.10",
    "dva": "^2.1.0",
    "dva-loading": "^1.0.4",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.2",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-scripts": "1.0.17",
    "redux-logger": "^3.0.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,less}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "proxy": {
    "/api": {
      "target": "http://jsonplaceholder.typicode.com/",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api": ""
      }
    }
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-less-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-less-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "PORT=8999 npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired build",
    "build": "npm-run-all build-css build-js",
    "web:s": "serve -s build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "autoprefixer": "^7.2.1",
    "babel-plugin-dva-hmr": "^0.4.0",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.3.5",
    "serve": "^6.4.1",
    "source-map-explorer": "^1.5.0"
  }
}
